{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev:api": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build:api": "tsc",
    "start:api": "node dist/server.js",
    "migrate:create": "node-pg-migrate create",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/domains/files/workers/file-processing.worker.ts",
    "worker:build": "tsc",
    "worker:start": "node dist/domains/files/workers/file-processing.worker.js",
    "dev": "concurrently \"npm run dev:api\" \"npm run worker:dev\"",
    "build": "npm run build:api && npm run worker:build",
    "start": "concurrently \"npm run start:api\" \"npm run worker:start\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "generate-proto": "grpc_tools_node_protoc   --js_out=import_style=commonjs,binary:./src/infrastructure/external-services/grpc/proto   --grpc_out=grpc_js:./src/infrastructure/external-services/grpc/proto   --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts   --ts_out=grpc_js:./src/infrastructure/external-services/grpc/proto   -I ./proto   ./proto/sanitizer.proto",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "postinstall": "echo '⚠️  Security Warning: Only install trusted packages. Avoid similarly named untrusted packages like core-pino, pino-final, or other malicious lookalikes.'"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "description": "",
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/google-protobuf": "^3.15.12",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/opossum": "^8.1.9",
    "@types/pdf-parse": "^1.1.5",
    "@types/pino": "^7.0.4",
    "@types/turndown": "^5.0.5",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "grpc-tools": "^1.13.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "protoc-gen-ts": "^0.8.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@huggingface/inference": "^4.7.1",
    "@mozilla/readability": "^0.6.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@xenova/transformers": "^2.17.2",
    "async-retry": "^1.3.3",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.0",
    "cheerio": "^1.1.2",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "file-type": "^21.0.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "mammoth": "^1.10.0",
    "minio": "^8.0.5",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-pg-migrate": "^8.0.3",
    "opossum": "^8.2.0",
    "p-limit": "^7.1.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pino": "9.11.0",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^7.3.0",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "turndown": "^7.2.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
