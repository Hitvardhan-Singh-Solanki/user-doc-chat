{
  "name": "backend",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "dev:api": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build:api": "tsc",
    "start:api": "node dist/main.js",
    "migrate:create": "node-pg-migrate create",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/workers/user-document-consumer.worker.ts",
    "worker:build": "tsc",
    "worker:start": "node dist/workers/user-document-consumer.worker.js",
    "dev": "concurrently \"npm run dev:api\" \"npm run worker:dev\"",
    "build": "npm run build:api && npm run worker:build",
    "start": "concurrently \"npm run start:api\" \"npm run worker:start\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --max-warnings 0 --fix",
    "generate-proto": "grpc_tools_node_protoc --ts_out=src/proto --js_out=import_style=commonjs,binary:src/proto --grpc_out=src/proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin -I ./proto ./proto/*.proto"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/google-protobuf": "^3.15.12",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/turndown": "^5.0.5",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "grpc-tools": "^1.13.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@huggingface/inference": "^4.7.1",
    "@mozilla/readability": "^0.6.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@xenova/transformers": "^2.17.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.0",
    "cheerio": "^1.1.2",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "file-type": "^21.0.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "mammoth": "^1.10.0",
    "minio": "^8.0.5",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-pg-migrate": "^8.0.3",
    "p-limit": "^7.1.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^7.3.0",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "turndown": "^7.2.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.1.5"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
