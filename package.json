{
  "name": "backend",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "dev:api": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build:api": "tsc",
    "start:api": "node dist/main.js",
    "migrate:create": "node-pg-migrate create",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/workers/user-document-consumer.worker.ts",
    "worker:build": "tsc",
    "worker:start": "node dist/workers/user-document-consumer.worker.js",
    "dev": "concurrently \"npm run dev:api\" \"npm run worker:dev\"",
    "build": "npm run build:api && npm run worker:build",
    "start": "concurrently \"npm run start:api\" \"npm run worker:start\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/pdf-parse": "^1.1.5",
    "@vitest/coverage-v8": "^3.2.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@huggingface/inference": "^4.7.1",
    "@pinecone-database/pinecone": "^6.1.2",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.0",
    "cheerio": "^1.1.2",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "file-type": "^21.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "mammoth": "^1.10.0",
    "minio": "^8.0.5",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-pg-migrate": "^8.0.3",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
